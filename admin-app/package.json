{
  "name": "recipes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css --watch",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!your-module-name)/\"",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "build:tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css",
    "prebuild": "npm run build:tailwind",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "yarn build && netlify deploy --prod"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.14.19",
    "@mui/x-data-grid": "^6.18.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@uiball/loaders": "^1.2.6",
    "@vitejs/plugin-react": "^4.1.0",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.1",
    "js-cookie": "^3.0.5",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-icons": "^5.2.1",
    "react-redux": "^9.0.1",
    "react-router-dom": "^6.20.1",
    "react-select": "^5.7.0",
    "styled-components": "^6.1.1",
    "tailwind-styled-components": "2.2.0",
    "ts-debounce": "^4.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.42",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.2",
    "@types/react-redux": "^7.1.32",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^9.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.27.1",
    "react-select-event": "^5.5.1",
    "sass": "^1.69.5",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.2",
    "vite": "^5.0.5",
    "webpack": "^5.72.0"
  }
}
