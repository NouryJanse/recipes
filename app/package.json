{
  "name": "recipes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "prod": "react-scripts build && serve -s build",
    "tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css --watch",
    "test": "react-scripts test --transformIgnorePatterns \"node_modules/(?!your-module-name)/\"",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "lint": "eslint .",
    "build-storybook": "build-storybook -s public",
    "build:tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css",
    "prebuild": "npm run build:tailwind",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "deploy": "yarn build && netlify deploy --prod"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.1.2",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.4",
    "@mui/x-data-grid": "^5.17.19",
    "@reduxjs/toolkit": "^1.9.7",
    "@storybook/react": "^6.5.5",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@types/react-select": "^5.0.1",
    "@uiball/loaders": "^1.2.6",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.1",
    "react": "^18.1.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-dropzone": "^12.0.5",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "6",
    "react-scripts": "^5.0.0",
    "react-select": "^5.7.0",
    "styled-components": "^5.3.5",
    "tailwind-styled-components": "2.1.7",
    "ts-debounce": "^4.0.0",
    "typescript": "^4.6.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.5",
    "@storybook/addon-essentials": "^6.5.5",
    "@storybook/addon-interactions": "^6.5.5",
    "@storybook/addon-links": "^6.5.5",
    "@storybook/builder-webpack5": "^6.5.5",
    "@storybook/manager-webpack5": "^6.5.5",
    "@storybook/node-logger": "^6.5.5",
    "@storybook/preset-create-react-app": "^4.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/auth0": "^2.34.18",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.8",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-dom": "^18.0.2",
    "@types/react-redux": "^7.1.24",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "react-select-event": "^5.5.1",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.72.0"
  }
}
