{
  "name": "recipes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css --watch",
    "server": "nodemon ./server/index.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build:tailwind": "tailwindcss build -i src/styles/input.css -o src/styles/tailwind.output.css",
    "prebuild": "npm run build:tailwind"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.21.0",
    "@reduxjs/toolkit": "^1.8.1",
    "axios": "^0.26.1",
    "dayjs": "^1.11.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.20.5",
    "react-redux": "^8.0.1",
    "react-router-dom": "6",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.5",
    "tailwind-styled-components": "^2.1.7",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.21",
    "@storybook/addon-essentials": "^6.4.21",
    "@storybook/addon-interactions": "^6.4.21",
    "@storybook/addon-links": "^6.4.21",
    "@storybook/builder-webpack5": "^6.4.21",
    "@storybook/manager-webpack5": "^6.4.21",
    "@storybook/node-logger": "^6.4.21",
    "@storybook/preset-create-react-app": "^4.1.0",
    "@storybook/react": "^6.4.21",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^14.1.1",
    "@types/auth0": "^2.34.18",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.2",
    "@types/react-redux": "^7.1.24",
    "@types/redux-thunk": "^2.1.0",
    "@types/styled-components": "^5.1.25",
    "react-icons": "^4.3.1",
    "tailwindcss": "^3.0.24",
    "webpack": "^5.72.0"
  }
}
